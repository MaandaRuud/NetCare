@model DocWeb.Models.EF.Doctor
@{
    ViewBag.Title = "Doctor";
}

<h3>Doctor</h3>
<ul class="breadcrumb">
    <li>@Html.ActionLink("Home", "Index", "Home")</li>
    <li>Doctor</li>
</ul>
@using (Html.BeginForm())
{
    <div class="row">
        <label class="col-sm-12 col-md-4 col-lg-3">Title</label>
        <div class="col-sm-12 col-md-8 col-lg-9">
            @Html.TextBoxFor(m => m.Title.TitleDescr, new { @class = "form-control", disabled = "true" })
        </div>
    </div>
    <div class="row">
        <label class="col-sm-12 col-md-4 col-lg-3">Last name</label>
        <div class="col-sm-12 col-md-8 col-lg-9">
            @Html.TextBoxFor(m => m.Lastname, new { @class = "form-control", disabled = "true" })
        </div>
    </div>
    <div class="row">
        <label class="col-sm-12 col-md-4 col-lg-3">First name</label>
        <div class="col-sm-12 col-md-8 col-lg-9">
            @Html.TextBoxFor(m => m.Firstname, new { @class = "form-control", disabled = "true" })
        </div>
    </div>
    <div class="row">
        <label class="col-sm-12 col-md-4 col-lg-3">ID Number</label>
        <div class="col-sm-12 col-md-8 col-lg-9">
            @Html.TextBoxFor(m => m.IdNo, new { @class = "form-control", disabled = "true" })
        </div>
    </div>
    <div class="row">
        <label class="col-sm-12 col-md-4 col-lg-3">Practice Number</label>
        <div class="col-sm-12 col-md-8 col-lg-9">
            @Html.TextBoxFor(m => m.HPCSANo, new { @class = "form-control", disabled = "true" })
        </div>
    </div>
    <div class="row">
        <label class="col-sm-12 col-md-4 col-lg-3">Discipline</label>
        <div class="col-sm-12 col-md-8 col-lg-9">
            @Html.TextBoxFor(m => m.Discipline.DisciplineDescr, new { @class = "form-control", disabled = "true" })
        </div>
    </div>
    <div class="row">
        <label class="col-sm-12 col-md-4 col-lg-3">Region</label>
        <div class="col-sm-12 col-md-8 col-lg-9">
            @Html.TextBoxFor(m => m.Region.RegionName, new { @class = "form-control", disabled = "true" })
        </div>
    </div>
    <div class="row">
        <label class="col-sm-12 col-md-4 col-lg-3">Province</label>
        <div class="col-sm-12 col-md-8 col-lg-9">
            @Html.TextBoxFor(m => m.Province.ProvinceName, new { @class = "form-control", disabled = "true" })
        </div>
    </div>
    <div style="margin-bottom:50px"></div>
}

